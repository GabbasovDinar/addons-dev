<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <template id="theme_kit.custom_css">
            <style type="text/css">
            </style>
            <script type="text/javascript">
            </script>
        </template>
        <template id="webclient_bootstrap" inherit_id="web.webclient_bootstrap">
            <xpath expr="//script[2]" position="before">
                <t t-call="theme_kit.custom_css"/>
            </xpath>
        </template>
        <template id="theme_kit.dummy">
        </template>
        <template id="assets_backend" name="color generator assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/theme_kit/static/lib/js/color-scheme.js"></script>
                <script type="text/javascript" src="/theme_kit/static/src/js/color-generator.js"></script>
            </xpath>
            <xpath expr="//link[last()]" position="after">
                <link rel='stylesheet' href='/theme_kit/static/src/css/generator-style.css'/>
            </xpath>
        </template>
    </data>
</odoo>
