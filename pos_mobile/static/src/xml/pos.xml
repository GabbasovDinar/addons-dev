<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-extend="ProductScreenWidget">
        <t t-jquery="div[class='product-screen screen']">
            $(this).addClass('swipe');
        </t>
    </t>

    <t t-extend="Chrome">
        <t t-jquery="div[class='subwindow-container-fix screens']">
            $(this).addClass('swipe').attr('id', 'mySwipe');
        </t>
    </t>

</templates>
