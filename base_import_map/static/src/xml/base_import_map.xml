<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="ImportView">
        <t t-jquery=".oe_import_tracking" t-operation="before">
            <input type="checkbox" class="oe_import_set_settings"
                   id="oe_import_set_settings" />
            <label for="oe_import_set_settings">Save Settings</label>

            <input type="text" class="oe_import_settings_name"
                   id="oe_import_settings_name" placeholder="Input setting name" />
        </t>
    </t>
</templates>
